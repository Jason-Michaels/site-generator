{
  "version": 2,
  "builds": [
      { "src": "package.json",
        "use": "@now/static-build",
        "config": { "distDir": "frontend/mysite.io/public" }
      },
      {
            "src": "index.py",
            "use": "@now/python",
            "config": { "maxLambdaSize": "25mb" }
      }
   ],
   "routes": [
        { "src": "/custom-page", "headers": {"cache-control": "s-maxage=1000"}, "dest": "/index.html" },
        {
            "src": "(.*)",
            "dest": "index.py"
        }
    ]
}